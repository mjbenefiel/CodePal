  "bugs": {
    "url": "https://github.com/tblukens/CodePal/issues"
  },
  "homepage": "https://github.com/tblukens/CodePal#readme",
  "dependencies": {
    "auth0-js": "^9.9.0",
    "axios": "^0.18.0",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.2",
    "passport": "^0.4.0",
    "passport-local-mongoose": "^5.0.1",
    "request": "^2.88.0",
    "socket.io": "^2.0.3",
    "underscore": "^1.9.1",
    "uuid": "^3.3.2"
  },
